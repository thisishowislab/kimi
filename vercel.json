{
  "version": 2,
  "rewrites": [
    {
      "source": "/product/:slug",
      "destination": "/product.html?slug=:slug"
    },
    {
      "source": "/tour/:slug",
      "destination": "/tour.html?slug=:slug"
    },
    {
      "source": "/field-notes/:slug",
      "destination": "/post.html?slug=:slug"
    },
    {
      "source": "/faq",
      "destination": "/faq.html"
    },
    {
      "source": "/faq/",
      "destination": "/faq.html"
    }
  ]
}
